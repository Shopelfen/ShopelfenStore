        "manufacturerLink": {
            "de-DE": "https://www.shopelfen.de",
            "en-GB": "https://www.shopelfen.de"
        },
        "supportLink": {
            "de-DE": "https://www.shopelfen.de/support",
            "en-GB": "https://www.shopelfen.com/support"
        }
    },
    "autoload": {
        "psr-4": {
            "ShopelfenStore\\": "src/"
        }
